{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "sku": {
      "type": "uid",
      "required": true,
      "default": "CQY-"
    },
    "stock": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "XS",
        "S",
        "M",
        "L",
        "XL",
        "XXL"
      ],
      "required": true
    },
    "prices": {
      "type": "component",
      "repeatable": true,
      "component": "product.price",
      "required": true
    },
    "design": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::design.design",
      "inversedBy": "products"
    },
    "title": {
      "type": "string",
      "required": true
    }
  }
}
