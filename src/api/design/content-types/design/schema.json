{
  "kind": "collectionType",
  "collectionName": "designs",
  "info": {
    "singularName": "design",
    "pluralName": "designs",
    "displayName": "Design",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "forwards": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "backwards": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "color": {
      "type": "string",
      "default": "#CCCCCC",
      "regex": "^#(?:[0-9a-fA-F]{3}){1,2}$",
      "unique": true,
      "maxLength": 7,
      "minLength": 7,
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "design"
    },
    "material": {
      "type": "string"
    }
  }
}
